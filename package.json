{
  "name": "superinvestoren",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fetch13f": "node --experimental-json-modules scripts/fetch13f.js",
    "fetchNportP": "node --experimental-json-modules scripts/fetchNportP.js",
    "fetch-torray": "node --experimental-json-modules scripts/scrapeDataromaTorray.js",
    "scrape-dataroma": "node scripts/scrapeDataromaAll.js",
    "scrape-dataroma:all": "node scripts/scrapeDataromaAll.js all",
    "scrape-dataroma:torray": "node scripts/scrapeDataromaAll.js torray",
    "scrape-dataroma:pabrai": "node scripts/scrapeDataromaAll.js pabrai",
    "scrape-dataroma:akre": "node scripts/scrapeDataromaAll.js akre",
    "update-holdings": "node scripts/updateHoldingsIndex.js",
    "sync-all": "npm run scrape-dataroma:all && npm run update-holdings",
    "sync-investor": "node scripts/scrapeDataromaAll.js",
    "update-stocks": "ts-node scripts/update-stocks.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "fetch:nportp": "node scripts/fetchNportP.js",
    "fetch:holdings": "node scripts/fetchHoldings.js",
    "delete:testusers": "node scripts/delete-test-users.js",
    "download-logos": "node scripts/downloadLogos.js",
    "setup-logos": "mkdir -p public/logos && node scripts/downloadLogos.js",
    "setup:data": "mkdir -p public/data && mkdir -p src/data",
    "scrape:realtime": "npm run setup:data && node scripts/scrapeDataromaRealTime.js && cp src/data/realtime-activity.json public/data/ 2>/dev/null || echo 'File copy skipped'",
    "dev:scrape": "npm run scrape:realtime && npm run dev",
    "rag:check": "npx tsx scripts/statusRAG-simple.ts",
    "rag:test": "npx tsx scripts/setupRAG.ts --test",
    "rag:clean": "npx tsx scripts/cleanRAG.ts",
    "rag:setup": "tsx scripts/setupRAG.ts",
    "rag:status": "tsx scripts/statusRAG.ts",
    "rag:ingest": "tsx scripts/ingestRAG.ts",
    "rag:check-env": "tsx scripts/checkEnv.ts",
    "rag:update": "tsx scripts/updateRAG.ts",
    "rag:setup-test": "tsx scripts/setupRAG.ts --test",
    "rag:ingest-major": "tsx scripts/ingestRAG.ts AAPL MSFT GOOGL AMZN TSLA NVDA META --full",
    "rag:update-daily": "tsx scripts/updateRAG.ts",
    "update-sectors": "tsx scripts/updateSectors.ts",
    "update-sectors:prod": "NODE_ENV=production tsx scripts/updateSectors.ts"
  },
